datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

model User {
    id              Int         @id @default(autoincrement())
    username        String      @unique
    email           String      @unique
    password        String
    name            String
    cpf             String
    birth           DateTime
    phone           String      
    pronoun         String

    image           String?     @db.Text
    

    googleId        String?
    googleToken     String?     @db.Text

}